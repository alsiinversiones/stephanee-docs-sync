  "paths": {
    "/update-doc": {
      "post": {
        "summary": "Actualizar contenido en Google Docs",
        "operationId": "updateDoc",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "documentId": { "type": "string" },
                  "content": { "type": "string" }
                },
                "required": ["documentId", "content"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Actualización realizada con éxito"
          }
        }
      }
    }
  }
}
